{
    "version" : 2,
    "builds" : [
        {
            "src" : "app.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src": "\/docs(.*)",
            "dest": "app.js"
        },
        {
            "src" : "\\/(.*)",
            "dest" : "app.js"
        }
    ]
}